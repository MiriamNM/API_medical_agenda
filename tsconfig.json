{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "strict": false,
        "outDir": "./dist",
        "rootDir": "./src",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "include": ["src/**/*.ts"]
}